{
  "name": "@splitsoftware/splitio-cache",
  "version": "0.10.0",
  "description": "Split Cache",
  "author": "Facundo Cabrera <facundo@split.io>",
  "homepage": "https://github.com/splitio/javascript-client",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/splitio/javascript-client/tree/master/packages/splitio-cache"
  },
  "main": "lib/node.js",
  "browser": "lib/browser.js",
  "scripts": {
    "build": "babel es6 --out-dir lib --source-maps",
    "build-tests": "babel test/es6 --out-dir test/lib --source-maps",
    "watch": "babel es6 --out-dir lib --source-maps --watch",
    "watch-test": "babel test/es6 --out-dir test/lib --source-maps --watch",
    "test": "tape test/lib/**/*.js"
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.0",
    "tape": "^4.2.2",
    "@splitsoftware/splitio-engine": "^0.10.1"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "jscs": "^2.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "splitio",
    "sdk"
  ]
}
